{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "version": "1.0.0",
  "name": "@nodejs-loaders/parse-filename",
  "description": "@nodejs-loaders utilities to parse filenames",
  "exports": "./parse-filename.mjs",
  "publish": {
    "include": [
      "./*.mjs",
      "./*.d.mts",
      "./*.d.mts.map"
    ],
    "exclude": [
      "!./*.mjs",
      "!./*.d.mts",
      "!./*.d.mts.map",
      "./*.bench.mjs",
      "./*.test.mjs",
      "./*.spec.mjs",
      "./fixtures/**/*"
    ]
  }
}
